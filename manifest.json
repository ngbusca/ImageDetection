{

  "manifest_version": 2,
  "name": "Image Detection",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",

  "background":{
      "scripts":["tfjs", "coco-ssd", "load-model.js"]
  },

  "content_scripts": [
    {
      "matches": ["https://thequasar.net/*/","https://www.perfil.com/*","https://www.nytimes.com/*"],
      "js": ["jquery-3.5.1.min.js","imagedetection.js"]
    }
  ],
  "permissions":[
      "https://*/*",
      "webRequest"
  ]

}

